<?xml version='1.0' encoding='utf-8'?>
<widget id="com.quranmemo.kelashijrah" version="1.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Kelas Hijrah</name>
    <description>Komunitas Penghafal Al-Quran Online Kelas Hijrah</description>
    <author email="quranmemo.id@gmail.com" href="http://quranmemo.id">Team QuranMemo.id</author>
    <content src="index.html" />
    <icon src="www/icon/icon-xxxhdpi.png" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
