<?xml version='1.0' encoding='utf-8'?>
<widget id="com.ndeztea.quranmemocommunity" version="1.8.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>QuranMemo Community</name>
    <description>Komunitas Penghafal Al-Quran Online dan tools untuk menghafal Al-Quran Mandiri</description>
    <author email="quranmemo.id@gmail.com" href="http://quranmemo.id">Team QuranMemo.id</author>
    <content src="index.html" />
    <icon src="www/icon/icon-xxxhdpi.png" />
    <access origin="*" />
